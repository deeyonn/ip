<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
         minHeight="-Infinity"
         minWidth="-Infinity" prefWidth="512.0" style="-fx-padding: 8px; -fx-spacing: 8px;"
         type="HBox" xmlns="http://javafx.com/javafx/8.0.171">
    <Label fx:id="dialog" alignment="TOP_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="400.0" prefWidth="400.0"
           style="-fx-background-color: #fcba03; -fx-background-radius: 8px; -fx-word-wrap: break-word;"
           textFill="WHITE" wrapText="true" HBox.hgrow="ALWAYS">
        <font>
            <Font name="Monaco" size="12.0"/>
        </font>
        <HBox.margin>
            <Insets/>
        </HBox.margin>
        <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
        </padding>
    </Label>
    <ImageView fx:id="displayPicture" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
        <HBox.margin>
            <Insets/>
        </HBox.margin>
    </ImageView>
</fx:root>